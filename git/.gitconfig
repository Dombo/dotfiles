[user]
	signingkey = F55FEE1F
	email = dom.hutton@gmail.com
	name = Dom Hutton
[commit]
	gpgsign = true
[alias]
	up = pull --rebase --autostash
	set-upstream = \
       !git branch \
           --set-upstream-to=origin/`git symbolic-ref --short HEAD`
[pull]
	rebase = true
[rebase]
	autoStash = true
[merge]
	tool = vscode
[mergetool "vscode"]
	cmd = "code --wait "
[diff]
	tool = vscode
[difftool "vscode"]
	cmd = "code --wait --diff  "
[push]
	default = current
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
